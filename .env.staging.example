# Staging PostgreSQL
POSTGRES_DB=smartpark_staging
POSTGRES_USER=smartpark_staging
POSTGRES_PASSWORD=change-me-staging

# Shared API auth
API_KEYS=change-me-staging-api-key

# Backend staging behavior
BACKEND_LOG_LEVEL=INFO
API_RATE_LIMIT=1500
API_RATE_LIMIT_WINDOW_SECONDS=60
CORS_ALLOW_ORIGINS=https://staging.your-domain.com

# Vision staging behavior
VISION_LOG_LEVEL=INFO
